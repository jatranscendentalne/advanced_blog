<h2>All articles</h2>

<h1>Listing articles</h1>


<p><%= link_to "New article", new_article_path %></p>

<table>
  <tr>
    <th>Id</th>
    <th>Title</th>
    <th>Action</th>
  </tr>
  <% @articles.each do |article| %>
  <tr>
    <td><%= article.id %></td>
    <td><%= article.title %></td>
    <td><%= link_to 'Show', article_path(article) %></td>
  </tr>
  <% end %>
</table>
